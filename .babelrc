{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage" ,
        "targets":{
          "chrome":"67"
        }
      }
    ],
    ["@babel/preset-react"]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 2,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ]
  ]
}